{
  "name": "farmers-market-graphql",
  "description": "GraphQL",
  "repository": "github:ben-walker/farmers-market-graphql",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "tsc",
    "dev": "./scripts/dev.sh",
    "dev:server": "ts-node-dev --exit-child --respawn src",
    "generate:prisma": "prisma generate",
    "generate:schema": "ts-node ./scripts/emit-schema",
    "lint": "eslint . --ext .ts",
    "start": "node build/src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "apollo-server-express": "^3.0.2",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/graphql-fields": "^1.3.4",
    "@types/node": "^16.4.4",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "concurrently": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.3.2",
    "prisma": "^2.27.0",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typegraphql-prisma": "^0.14.7",
    "typescript": "^4.3.5"
  }
}
