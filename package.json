{
  "name": "farmers-market-graphql",
  "description": "Farmers market GraphQL API",
  "repository": "github:ben-walker/farmers-market-graphql",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "tsc",
    "dev": "./scripts/dev.sh",
    "graphql:generate": "ts-node scripts/emit-schema",
    "lint": "eslint . --ext .ts",
    "prisma:db:sync": "prisma db push && prisma db seed --preview-feature",
    "prisma:dev": "chokidar \"prisma\" --command \"npm run prisma:db:sync\"",
    "start": "node build/src",
    "start:dev": "ts-node-dev --exit-child --respawn src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^2.30.0",
    "apollo-server-express": "^3.1.2",
    "argon2": "^0.28.2",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0",
    "ioredis": "^4.27.7",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.17",
    "@types/express-session": "^1.17.4",
    "@types/graphql-fields": "^1.3.4",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.3.2",
    "prisma": "^2.30.0",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "typegraphql-prisma": "^0.15.0",
    "typescript": "^4.3.5"
  }
}
